{
    "name": "mrsuh/json-validation-bundle",
    "description": "This bundle provides a way to validate JSON in request/response against a schema",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "symfony/config": ">=3.0",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "justinrainbow/json-schema": ">=5.2",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anton Sukhachev",
            "email": "mrsuh6@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": { "Mrsuh\\JsonValidationBundle\\": "" }
    }
}
