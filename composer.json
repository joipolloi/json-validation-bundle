    "type": "symfony-bundle",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "symfony/config": ">=3.0",
        "symfony/http-foundation": ">=3.2",
        "justinrainbow/json-schema": ">=5.2",
        "sensio/framework-extra-bundle": ">=3.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "license": "MIT",
    "authors": [
        {
